<%- contentFor('body') %>
<h1>Quản lý Người dùng</h1>
<table class="table">
  <thead>
    <tr>
      <th>Tên người dùng</th>
      <th>Email</th>
      <th>Vai trò</th>
    </tr>
  </thead>
  <tbody>
    <% users.forEach(user => { %>
    <tr>
      <td><%= user.username %></td>
      <td><%= user.email %></td>
      <td><%= user.role %></td>
    </tr>
    <% }) %>
  </tbody>
</table>
